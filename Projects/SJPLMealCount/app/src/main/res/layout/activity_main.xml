<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.harambehackers.sjplmealcount.MainActivity">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tully Community Branch Library"
        android:id="@+id/buttont"
        android:layout_below="@+id/buttona"
        android:layout_centerHorizontal="true"
        android:onClick="handleButtonClicked"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Biblioteca Latioamerica"
        android:id="@+id/buttonb"
        android:layout_below="@+id/buttone"
        android:layout_centerHorizontal="true"
        android:onClick="handleButtonClicked"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HillView Branch"
        android:id="@+id/buttonh"
        android:layout_below="@+id/buttonb"
        android:layout_centerHorizontal="true"
        android:onClick="handleButtonClicked"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alum Rock"
        android:id="@+id/buttona"
        android:layout_below="@+id/buttonj"
        android:layout_centerHorizontal="true"
        android:onClick="handleButtonClicked"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Joyce Ellington"
        android:id="@+id/buttonj"
        android:layout_below="@+id/buttonh"
        android:layout_centerHorizontal="true"
        android:onClick="handleButtonClicked"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Educational Park"
        android:id="@+id/buttone"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="86dp"
        android:onClick="handleButtonClicked"/>
</RelativeLayout>
